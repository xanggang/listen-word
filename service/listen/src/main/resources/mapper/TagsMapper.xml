<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.listen.mapper.TagsMapper">

    <resultMap type="com.listen.entity.Tags" id="TagsMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="stationcount" column="stationcount" jdbcType="INTEGER"/>
    </resultMap>

    <select id="page" resultMap="TagsMap">
        select * from tags order by stationcount desc
    </select>

</mapper>
